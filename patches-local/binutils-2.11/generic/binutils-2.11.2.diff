+  do									\
+    {									\
+      char regname[20];							\
+									\
+      unw_print_abreg (regname, abreg);					\
+      printf ("\t%s:restore_p(qp=p%u,t=%lu,reg=%s)\n",			\
+	      fmt, qp, (unsigned long) t, regname);			\
+    }									\
+  while (0)
+
+#define UNW_DEC_SPILL_REG_P(fmt, qp, t, abreg, x, ytreg, arg)		\
+  do									\
