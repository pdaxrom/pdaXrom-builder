-      h1 = coff_link_hash_lookup (coff_hash_table (info),
-				  ".idata$5", false, false, true);
-      pe_data (abfd)->pe_opthdr.DataDirectory[12].VirtualAddress =
-	(h1->root.u.def.value
-	 + h1->root.u.def.section->output_section->vma
-	 + h1->root.u.def.section->output_offset);
-      h1 = coff_link_hash_lookup (coff_hash_table (info),
