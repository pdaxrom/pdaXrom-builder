+		  "mvn r5, r5 \n\t"
+		  "mvn r7, r7 \n\t"
+		  "tst r6, #0x100 \n\t"
+		  "movne r6, r5, lsr #24 \n\t"
+		  "tst r8, #0x100 \n\t"
+		  "movne r8, r7, lsr #24 \n\t"
