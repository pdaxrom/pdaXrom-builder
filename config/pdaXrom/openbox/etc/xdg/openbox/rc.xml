<?xml version="1.0" ?>
<!-- Do not edit this file, it will be overwritten on install.
        Copy the file to $HOME/.config/openbox/ instead. --><openbox_config xmlns="http://openbox.org/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://openbox.org/                 file:///usr/share/openbox/rc.xsd">

<resistance>
  <strength>10</strength>
  <screen_edge_strength>20</screen_edge_strength>
</resistance>

<focus>
  <focusNew>yes</focusNew>
  <followMouse>no</followMouse>
  <focusDelay>0</focusDelay>
  <raiseOnFocus>no</raiseOnFocus>
</focus>

<theme>
  <name>Artwiz</name>
  <titlelayout>NLIMC</titlelayout>
</theme>

<placement>
  <policy>Smart</policy>
</placement>

<desktops>
  <number>4</number>
  <names>
    <name>one</name>
    <name>two</name>
    <name>three</name>
    <name>four</name>
  </names>
</desktops>

<resize>
  <drawContents>yes</drawContents>
</resize>

<dock>
  <position>TopLeft</position>
  <stacking>Top</stacking>
  <direction>Vertical</direction>
  <floatingX>0</floatingX>
  <floatingY>0</floatingY>
  <autoHide>no</autoHide>
  <hideDelay>300</hideDelay>
  <moveButton>A-Left</moveButton>
</dock>

<keyboard>
  <chainQuitKey>C-g</chainQuitKey>

  <keybind key="A-F10">
    <action name="MaximizeFull"/>
  </keybind>
  <keybind key="A-F5">
    <action name="UnmaximizeFull"/>
  </keybind>
  <keybind key="A-F12">
    <action name="ToggleShade"/>
  </keybind>
  <keybind key="C-A-Left">
    <action name="DesktopLeft"><wrap>no</wrap></action>
  </keybind>
  <keybind key="C-A-Right">
    <action name="DesktopRight"><wrap>no</wrap></action>
  </keybind>
  <keybind key="C-A-Up">
    <action name="DesktopUp"><wrap>no</wrap></action>
  </keybind>
  <keybind key="C-A-Down">
    <action name="DesktopDown"><wrap>no</wrap></action>
  </keybind>
  <keybind key="S-A-Left">
    <action name="SendToDesktopLeft"><wrap>no</wrap></action>
  </keybind>
  <keybind key="S-A-Right">
    <action name="SendToDesktopRight"><wrap>no</wrap></action>
  </keybind>
  <keybind key="S-A-Up">
    <action name="SendToDesktopUp"><wrap>no</wrap></action>
  </keybind>
  <keybind key="S-A-Down">
    <action name="SendToDesktopDown"><wrap>no</wrap></action>
  </keybind>
  <keybind key="C-A-d">
    <action name="ToggleShowDesktop"/>
  </keybind>
  <keybind key="A-F4">
    <action name="Close"/>
  </keybind>
  <keybind key="A-Tab">
    <action name="NextWindow"/>
  </keybind>
  <keybind key="A-S-Tab">
    <action name="PreviousWindow"/>
  </keybind>
  <keybind key="A-F7">
    <action name="Move"/>
  </keybind>
  <keybind key="A-F8">
    <action name="Resize"/>
  </keybind>
  <keybind key="A-F9">
    <action name="Iconify"/>
  </keybind>
  <keybind key="C-A-m">
    <action name="ToggleMaximizeFull"/>
  </keybind>
  <keybind key="Mod2-C-A-Down">
    <action name="MoveRelativeVert"><delta>+1</delta></action>
  </keybind>
  <keybind key="Mod2-C-A-Up">
    <action name="MoveRelativeVert"><delta>-1</delta></action>
  </keybind>
  <keybind key="Mod2-C-A-Left">
    <action name="MoveRelativeHorz"><delta>-1</delta></action>
  </keybind>
  <keybind key="Mod2-C-A-Right">
    <action name="MoveRelativeHorz"><delta>+1</delta></action>
  </keybind>
  <keybind key="Mod2-1">
    <action name="Execute"><execute>xrandr -s 0</execute></action>
  </keybind>
  <keybind key="Mod2-2">
    <action name="Execute"><execute>xrandr -s 1</execute></action>
  </keybind>
  <keybind key="Mod2-3">
    <action name="Execute"><execute>setfl -</execute></action>
  </keybind>
  <keybind key="Mod2-4">
    <action name="Execute"><execute>setfl +</execute></action>
  </keybind>
  <keybind key="A-space">
    <action name="ShowMenu"><menu>client-menu</menu></action>
  </keybind>
  <keybind key="XF86RotateWindows">
    <action name="Execute"><execute>chkhinge</execute></action>
  </keybind>
  <keybind key="XF86Launch0">
    <action name="Execute"><execute>dillo</execute></action>
  </keybind>
  <keybind key="Mod2-XF86Launch0">
    <action name="Execute"><execute>firefox</execute></action>
  </keybind>
  <keybind key="XF86Launch1">
    <action name="Execute"><execute>sylpheed</execute></action>
  </keybind>
  <keybind key="Mod2-XF86Launch1">
    <action name="Execute"><execute>thunderbird</execute></action>
  </keybind>
  <keybind key="XF86Launch2">
    <action name="Execute"><execute>kapi</execute></action>
  </keybind>
  <keybind key="Mod2-XF86Launch2">
    <action name="Execute"><execute/></action>
  </keybind>
  <keybind key="XF86Launch3">
    <action name="Execute"><execute>kopi</execute></action>
  </keybind>
  <keybind key="XF86Launch4">
    <action name="Execute"><execute>xmms</execute></action>
  </keybind>
  <keybind key="Mod2-XF86Launch4">
    <action name="ToggleShowDesktop"/>
  </keybind>
  <keybind key="XF86Start">
    <action name="Execute"><execute>mb-start-button</execute></action>
  </keybind>
  <keybind key="XF86LaunchA">
    <action name="Execute"><execute></execute></action>
  </keybind>
  <keybind key="XF86LaunchB">
    <action name="Execute"><execute></execute></action>
  </keybind>
  <keybind key="XF86LaunchC">
    <action name="Execute"><execute>sylpheed</execute></action>
  </keybind>
  <keybind key="XF86LaunchD">
    <action name="Execute"><execute></execute></action>
  </keybind>
  <keybind key="S-XF86LaunchD">
    <action name="Execute"><execute>xset dpms force off</execute></action>
  </keybind>
  <keybind key="XF86AudioPlay">
    <action name="Execute"><execute>xmms -t</execute></action>
  </keybind>
  <keybind key="XF86AudioStop">
    <action name="Execute"><execute>xmms -s</execute></action>
  </keybind>
  <keybind key="XF86AudioPrev">
    <action name="Execute"><execute>xmms -r</execute></action>
  </keybind>  
  <keybind key="XF86AudioNext">
    <action name="Execute"><execute>xmms -f</execute></action>
  </keybind>
  <keybind key="XF86AudioLowerVolume">
    <action name="Execute"><execute>xmms --vol-down</execute></action>
  </keybind>  
  <keybind key="XF86AudioRaiseVolume">
    <action name="Execute"><execute>xmms --vol-up</execute></action>
  </keybind>
</keyboard>

<mouse>
  <dragThreshold>3</dragThreshold>
  <doubleClickTime>200</doubleClickTime>

  <context name="Frame">
    <mousebind action="Drag" button="A-Left">
      <action name="Move"/>
    </mousebind>
    <mousebind action="Click" button="A-Left">
      <action name="Raise"/>
    </mousebind>
    <mousebind action="Press" button="A-Left">
      <action name="Focus"/>
    </mousebind>
    <mousebind action="Drag" button="A-Middle">
      <action name="Resize"/>
    </mousebind> 
    <mousebind action="Click" button="A-Middle">
      <action name="Lower"/>
    </mousebind>
    <mousebind action="Press" button="A-Right">
      <action name="ShowMenu"><menu>client-menu</menu></action>
    </mousebind>
    <mousebind action="Click" button="A-Up">
      <action name="DesktopPrevious"/>
    </mousebind>
    <mousebind action="Click" button="A-Down">
      <action name="DesktopNext"/>
    </mousebind>
    <mousebind action="Click" button="C-A-Up">
      <action name="SendToDesktopPrevious"/>
    </mousebind>
    <mousebind action="Click" button="C-A-Down">
      <action name="SendToDesktopNext"/>
    </mousebind>
  </context>
  <context name="Titlebar">
    <mousebind action="Drag" button="Left">
      <action name="Move"/>
    </mousebind>
    <mousebind action="Click" button="Left">
      <action name="Raise"/>
    </mousebind>
    <mousebind action="Press" button="Left">
      <action name="Focus"/>
    </mousebind>
    <mousebind action="DoubleClick" button="Left">
      <action name="ToggleShade"/>
    </mousebind>
    <mousebind action="Press" button="Middle">
      <action name="Lower"/>
    </mousebind>
    <mousebind action="Click" button="Up">
      <action name="Shade"/>
    </mousebind>
    <mousebind action="Click" button="Down">
      <action name="Unshade"/>
    </mousebind>
    <mousebind action="Press" button="Right">
      <action name="ShowMenu"><menu>client-menu</menu></action>
    </mousebind>
  </context>
  <context name="Handle">
    <mousebind action="Drag" button="Left">
      <action name="Move"/>
    </mousebind>
    <mousebind action="Click" button="Left">
      <action name="Raise"/>
    </mousebind>
    <mousebind action="Press" button="Left">
      <action name="Focus"/>
    </mousebind>
    <mousebind action="Press" button="Middle">
      <action name="Lower"/>
    </mousebind>
  </context>
  <context name="BLCorner">
    <mousebind action="Drag" button="Left">
      <action name="Resize"/>
    </mousebind>
    <mousebind action="Press" button="Left">
      <action name="Focus"/>
    </mousebind>
  </context>
  <context name="BRCorner">
    <mousebind action="Drag" button="Left">
      <action name="Resize"/>
    </mousebind>
    <mousebind action="Press" button="Left">
      <action name="Focus"/>
    </mousebind>
  </context>
  <context name="TLCorner">
    <mousebind action="Drag" button="Left">
      <action name="Resize"/>
    </mousebind>
    <mousebind action="Press" button="Left">
      <action name="Focus"/>
    </mousebind>
  </context>
  <context name="TRCorner">
    <mousebind action="Drag" button="Left">
      <action name="Resize"/>
    </mousebind>
    <mousebind action="Press" button="Left">
      <action name="Focus"/>
    </mousebind>
  </context>
  <context name="Client">
    <mousebind action="Press" button="Left">
      <action name="Focus"/>
      <action name="Raise"/>
    </mousebind>
    <mousebind action="Press" button="Middle">
      <action name="Focus"/>
    </mousebind>
    <mousebind action="Press" button="Right">
      <action name="Focus"/>
    </mousebind>
  </context>
  <context name="Icon">
    <mousebind action="Press" button="Left">
      <action name="Focus"/>
    </mousebind>
    <mousebind action="Press" button="Right">
      <action name="ShowMenu"><menu>client-menu</menu></action>
    </mousebind>
    <mousebind action="Press" button="Left">
      <action name="ShowMenu"><menu>client-menu</menu></action>
    </mousebind>
  </context>
  <context name="AllDesktops">
    <mousebind action="Press" button="Left">
      <action name="Focus"/>
    </mousebind>
    <mousebind action="Click" button="Left">
      <action name="ToggleOmnipresent"/>
    </mousebind>
  </context>
  <context name="Shade">
    <mousebind action="Press" button="Left">
      <action name="Focus"/>
    </mousebind>
    <mousebind action="Click" button="Left">
      <action name="ToggleShade"/>
    </mousebind>
  </context>
  <context name="Iconify">
    <mousebind action="Press" button="Left">
      <action name="Focus"/>
    </mousebind>
    <mousebind action="Click" button="Left">
      <action name="Iconify"/>
    </mousebind>
  </context>
  <context name="Maximize">
    <mousebind action="Press" button="Left">
      <action name="Focus"/>
    </mousebind>
    <mousebind action="Press" button="Middle">
      <action name="Focus"/>
    </mousebind>
    <mousebind action="Press" button="Right">
      <action name="Focus"/>
    </mousebind>
    <mousebind action="Click" button="Left">
      <action name="ToggleMaximizeFull"/>
    </mousebind>
    <mousebind action="Click" button="Middle">
      <action name="ToggleMaximizeVert"/>
    </mousebind>
    <mousebind action="Click" button="Right">
      <action name="ToggleMaximizeHorz"/>
    </mousebind>
  </context>
  <context name="Close">
    <mousebind action="Press" button="Left">
      <action name="Focus"/>
    </mousebind>
    <mousebind action="Click" button="Left">
      <action name="Close"/>
    </mousebind>
  </context>
  <context name="Desktop">
    <mousebind action="Press" button="Up">
      <action name="DesktopPrevious"/>
    </mousebind>
    <mousebind action="Press" button="Down">
      <action name="DesktopNext"/>
    </mousebind>
    <mousebind action="Press" button="A-Up">
      <action name="DesktopPrevious"/>
    </mousebind>
    <mousebind action="Press" button="A-Down">
      <action name="DesktopNext"/>
    </mousebind>
    <mousebind action="Press" button="Left">
      <action name="Focus"/>
      <action name="Raise"/>
    </mousebind> 
    <mousebind action="Press" button="A-Left">
      <action name="ShowMenu"><menu>client-list-menu</menu></action>
    </mousebind> 
    <mousebind action="Press" button="C-Left">
      <action name="ShowMenu"><menu>root-menu</menu></action>
    </mousebind>
  </context>
  <context name="MoveResize">
    <mousebind action="Press" button="Up">
      <action name="DesktopPrevious"/>
    </mousebind>
    <mousebind action="Press" button="Down">
      <action name="DesktopNext"/>
    </mousebind>
    <mousebind action="Press" button="A-Up">
      <action name="DesktopPrevious"/>
    </mousebind>
    <mousebind action="Press" button="A-Down">
      <action name="DesktopNext"/>
    </mousebind>
  </context>
</mouse>

<menu>
  <!-- You can specify more than one menu file in here and they are all loaded,
       just don't make menu ids clash or, well, it'll be kind of pointless -->

  <!-- default menu file (or custom one in $HOME/.config/openbox/) -->
  <file>menu.xml</file>
</menu>

</openbox_config>